<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
  <com.google.android.gms.ads.nativead.NativeAdView
      android:layout_height="match_parent"
      android:layout_width="match_parent"
      android:layout_centerInParent="true"
      android:id="@+id/native_ad_view"
      >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:background="@drawable/gnt_outline_shape"
        >
      <androidx.constraintlayout.widget.ConstraintLayout
          android:id="@+id/background"
          android:layout_width="0dp"
          android:layout_height="0dp"
          android:layout_margin="@dimen/gnt_default_margin"
          app:layout_constraintEnd_toEndOf="parent"
          app:layout_constraintStart_toStartOf="parent"
          app:layout_constraintTop_toTopOf="parent"
          app:layout_constraintBottom_toBottomOf="parent"
          >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/data_panel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            >
          <TextView
              android:id="@+id/ad_notification_view"
              android:layout_width="@dimen/_20sdp"
              android:layout_height="@dimen/_20sdp"
              android:text="Ad"
              android:textColor="@color/gnt_white"
              android:textStyle="bold"
              android:textSize="@dimen/_10ssp"
              android:gravity="center"
              android:background="@drawable/gnt_rounded_corners_shape"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toTopOf="parent"
              />

          <ImageView
              android:id="@+id/icon"
              android:layout_width="@dimen/_65sdp"
              android:layout_height="@dimen/_65sdp"
              android:layout_marginTop="5dp"
              app:layout_constraintStart_toStartOf="parent"
              app:layout_constraintTop_toBottomOf="@+id/ad_notification_view"
              />
          <TextView
              android:id="@+id/primary"
              android:layout_height="wrap_content"
              android:layout_width="0dp"
              android:textStyle="bold"
              android:textSize="@dimen/_15ssp"
              android:textColor="@color/gnt_white"
              android:lines="1"
              app:layout_constraintVertical_chainStyle="packed"
              android:layout_marginStart="5dp"
              android:layout_marginEnd="5dp"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toEndOf="@+id/icon"
              app:layout_constraintTop_toTopOf="@+id/icon"
              />

          <TextView
              android:id="@+id/secondary"
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:textSize="@dimen/_12ssp"
              android:textColor="@color/gnt_white"
              android:lines="1"
              android:layout_marginTop="5dp"
              android:layout_marginStart="5dp"
              android:layout_marginEnd="5dp"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toEndOf="@id/icon"
              app:layout_constraintTop_toBottomOf="@+id/primary"
              />

          <TextView
              android:id="@+id/body"
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:paddingBottom="@dimen/gnt_default_margin"
              android:layout_margin="@dimen/_5sdp"
              android:textColor="@color/gnt_white"
              app:layout_constraintTop_toBottomOf="@id/secondary"
              app:layout_constraintStart_toEndOf="@+id/icon"
              app:layout_constraintEnd_toEndOf="parent"
              />


          <RatingBar
              android:id="@+id/rating_bar"
              android:layout_width="0dp"
              android:layout_height="@dimen/_40sdp"
              android:textSize="@dimen/_10ssp"
              android:textColor="@color/gnt_white"
              android:numStars="5"
              android:stepSize="0.1"
              android:lines="1"
              android:max="5"
              android:scaleX=".5"
              android:scaleY=".5"
              android:layout_marginStart="5dp"
              android:layout_marginEnd="5dp"
              app:layout_constraintEnd_toEndOf="parent"
              app:layout_constraintStart_toEndOf="@id/icon"
              app:layout_constraintTop_toBottomOf="@+id/body"
              />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <com.google.android.gms.ads.nativead.MediaView
            android:id="@+id/media_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/data_panel"
            app:layout_constraintBottom_toTopOf="@id/cta"
            />
        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/cta"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_40sdp"
            android:background="@drawable/gnt_bg_button"
            android:textColor="@color/gnt_black"
            android:textStyle="bold"
            android:lines="1"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            />


      </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

  </com.google.android.gms.ads.nativead.NativeAdView>

</merge>
